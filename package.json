{
  "name": "langquest",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "generate-icon-types": "ts-node ./scripts/generate-icon-types.ts",
    "generate-env": "npx tsx ./scripts/generate-env.ts",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint --ignore-pattern .gitignore",
    "lint:fix": "eslint --fix --ignore-pattern .gitignore",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "powersync:start": "docker compose -f supabase/docker-compose.yml --env-file .env.local up -w",
    "powersync:stop": "docker compose -f supabase/docker-compose.yml --env-file .env.local down",
    "env:start": "npm run supabase start && npm run powersync:start",
    "env:stop": "npm run powersync:stop && npm run supabase stop",
    "env": "npm run generate-env && npm run env:start && npm run env:stop",
    "env:clean": "npm run supabase:clean && npm run powersync:clean",
    "powersync:clean": "npm run powersync:stop && docker volume rm -f powersync_mongo_storage",
    "supabase:clean": "git clean -xdf supabase/*/.DS_Store && npm run supabase db reset && npm run supabase stop",
    "supabase": "npx supabase@beta",
    "supabase:serve-functions": "npm run supabase functions serve -- --no-verify-jwt --debug",
    "supabase:functions:deploy:send-email:dev": "npm run supabase functions deploy send-email -- --no-verify-jwt --project-ref hjsrpgqyobgfyeinrunx",
    "supabase:functions:deploy:send-email:prod": "npm run supabase functions deploy send-email -- --no-verify-jwt"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@expo/vector-icons": "^14.0.2",
    "@journeyapps/react-native-quick-sqlite": "^2.4.0",
    "@powersync/attachments": "^2.2.0",
    "@powersync/drizzle-driver": "^0.3.2",
    "@powersync/react-native": "^1.18.1",
    "@powersync/tanstack-react-query": "^0.0.20",
    "@powersync/web": "^1.15.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/drawer": "^6.0.2",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.6.4",
    "@supabase/supabase-js": "^2.46.1",
    "base64-arraybuffer": "^1.0.2",
    "crypto-js": "3.1.9-1",

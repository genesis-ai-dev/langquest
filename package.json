{
  "name": "langquest",
  "main": "expo-router/entry",
  "version": "1.3.2",
  "scripts": {
    "start": "expo start --dev-client",
    "generate-icon-types": "ts-node ./scripts/generate-icon-types.ts",
    "generate-env": "npx tsx ./scripts/generate-env.ts",
    "reset-project": "node ./scripts/reset-project.js",
    "rebuild-android": "git clean -xdf node_modules package-lock.json android ios && npm i && npx expo prebuild --platform android && cd android && gradlew clean && cd ..",
    "android": "expo run:android --device",
    "ios": "expo run:ios --device",
    "web": "npm run powersync:copy-assets && expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint --ignore-pattern .gitignore",
    "lint:fix": "eslint --fix --ignore-pattern .gitignore",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "powersync:start": "docker compose -f supabase/docker-compose.yml --env-file .env.local up -w",
    "powersync:stop": "docker compose -f supabase/docker-compose.yml --env-file .env.local down",
    "env:start": "npx supabase start && npm run powersync:start",
    "env:stop": "npm run powersync:stop && npx supabase stop",
    "env": "npm run generate-env && npm run env:start && npm run env:stop",
    "env:clean": "npm run supabase:clean && npm run powersync:clean",
    "powersync:copy-assets": "npx powersync-web copy-assets",
    "powersync:clean": "npm run powersync:stop && docker volume rm -f powersync_mongo_storage",
    "supabase:clean": "git clean -xdf supabase/**/.DS_Store && npx supabase db reset && npx supabase stop",
    "supabase:serve-functions": "npx supabase functions serve -- --no-verify-jwt --debug",
    "supabase:functions:deploy:send-email:dev": "npx supabase functions deploy send-email -- --no-verify-jwt --project-ref yjgdgsycxmlvaiuynlbv",
    "supabase:functions:deploy:send-email:prod": "npx supabase functions deploy send-email -- --no-verify-jwt --project-ref unsxkmlcyxgtgmtzfonb",
    "shadcn": "shadcn"
  },
  "jest": {
    "preset": "jest-expo"
  },

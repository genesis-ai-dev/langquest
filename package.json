    "typescript": "~5.3.3",
    "typescript-eslint": "^8.20.0"
  },
  "overrides": {
    "ajv": "^6.12.6",
    "schema-utils": "3.1.1"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native@~0.76.6",
        "react-native-reanimated@~3.16.1",
        "react-native-gesture-handler@~2.22.0",
        "react-native-screens@~4.8.0",
        "react-native-safe-area-context@~5.1.0",
        "react-native-webview@~13.13.1"
      ]
    }
  },
  "private": true

{
  "name": "langquest",
  "main": "expo-router/entry",
  "version": "1.2.0",
  "scripts": {
    "start": "expo start --dev-client",
    "generate-icon-types": "ts-node ./scripts/generate-icon-types.ts",
    "generate-env": "npx tsx ./scripts/generate-env.ts",
    "reset-project": "node ./scripts/reset-project.js",
    "rebuild-android": "git clean -xdf node_modules package-lock.json android ios && npm i && npx expo prebuild --platform android && cd android && gradlew clean && cd ..",
    "android": "expo run:android --device",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint --ignore-pattern .gitignore",
    "lint:fix": "eslint --fix --ignore-pattern .gitignore",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "powersync:start": "docker compose -f supabase/docker-compose.yml --env-file .env.local up -w",
    "powersync:stop": "docker compose -f supabase/docker-compose.yml --env-file .env.local down",
    "env:start": "npm run supabase start && npm run powersync:start",
    "env:stop": "npm run powersync:stop && npm run supabase stop",
    "env": "npm run generate-env && npm run env:start && npm run env:stop",
    "env:clean": "npm run supabase:clean && npm run powersync:clean",
    "powersync:clean": "npm run powersync:stop && docker volume rm -f powersync_mongo_storage",
    "supabase:clean": "git clean -xdf supabase/**/.DS_Store && npm run supabase db reset && npm run supabase stop",
    "supabase": "npx supabase",
    "supabase:migration:diff": "npm run supabase:migration:diff:prep && npm run supabase:migration:diff:run; npm run supabase:migration:diff:cleanup",
    "supabase:migration:diff:prep": "mv supabase/schemas/storage.sql supabase/schemas/storage.sql.bak 2>/dev/null || true",
    "supabase:migration:diff:run": "npx supabase@beta db diff -f",
    "supabase:migration:diff:cleanup": "mv supabase/schemas/storage.sql.bak supabase/schemas/storage.sql 2>/dev/null || true",
    "supabase:serve-functions": "npm run supabase functions serve -- --no-verify-jwt --debug",
    "supabase:functions:deploy:send-email:dev": "npm run supabase functions deploy send-email -- --no-verify-jwt --project-ref yjgdgsycxmlvaiuynlbv",
    "supabase:functions:deploy:send-email:prod": "npm run supabase functions deploy send-email -- --no-verify-jwt --project-ref unsxkmlcyxgtgmtzfonb"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@expo/vector-icons": "^14.0.2",
    "@op-engineering/op-sqlite": "^11.2.13",
    "@powersync/attachments": "^2.3.1",
    "@powersync/drizzle-driver": "^0.4.0",
    "@powersync/op-sqlite": "^0.5.3",
    "@powersync/react-native": "^1.19.3",
    "@powersync/tanstack-react-query": "^0.0.21",
    "@powersync/web": "^1.19.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.1.6",
    "@react-navigation/drawer": "^7.1.6",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.1.6",
    "@shopify/flash-list": "1.7.3",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/query-async-storage-persister": "^5.80.5",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-persist-client": "^5.80.5",
    "base64-arraybuffer": "^1.0.2",
    "crypto-js": "3.1.9-1",
    "eslint-config-expo": "~8.0.1",
    "expo": "^52.0.46",
    "expo-application": "~6.0.2",
    "expo-av": "~15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-json-utils": "^0.14.0",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-manifests": "^0.15.8",
    "expo-router": "~4.0.20",

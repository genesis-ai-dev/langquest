{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "developmentClient": true
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "autoIncrement": true,
      "channel": "preview",
      "environment": "preview",
      "distribution": "internal",
      "ios": {
        "distribution": "store"
      }
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true,
        "distribution": "internal"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production"
    },
    "production-simulator": {
      "extends": "production",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "ascAppId": "6752446665"
      }
    },
    "production": {
      "android": {
        "track": "production"
      },
      "ios": {
        "ascAppId": "6752446665"
      }
    }
  }
}

{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "cache": {
          "key": "android-native-v1"
        }
      },
      "prebuildCommand": "npx expo prebuild --platform android --clean",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {}
}

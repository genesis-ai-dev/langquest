{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "preview",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "development"
      }
    },
    "preview": {
      "autoIncrement": true,
      "channel": "preview",
      "environment": "preview",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "preview": {
      "android": {
        "track": "alpha"
      }
    }
  }
}

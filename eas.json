{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "development"
      },
      "developmentClient": true,
      "ios": {
        "simulator": true,
        "distribution": "store"
      }
    },
    "preview": {
      "autoIncrement": true,
      "channel": "preview",
      "environment": "preview",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_SITE_URL": "https://langquest.org",
        "EXPO_PUBLIC_APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "ascAppId": "6752446665"
      }
    },
    "production": {
      "android": {
        "track": "production"
      },
      "ios": {
        "ascAppId": "6752446665"
      }
    }
  }
}
